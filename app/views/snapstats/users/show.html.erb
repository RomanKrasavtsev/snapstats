
<div class="container animated fadeIn">

	<div class="page-header">
		<h1>Users Activity</h1>
	</div>

	<div class="row">
		<div class="col-lg-12">
			<table class="table">
				
				<% @users.each do |i| %>
				
					<tr>
						<td class="col-lg-5"> <%= i.email %> </td>
						<td class="col-lg-3"> <%= time_ago_in_words i.date, locale: :en %>  </td>
						<td> <%= i.path %>  </td>
					</tr>

				<% end %>

			</table>
		</div>
	</div>

</div>
